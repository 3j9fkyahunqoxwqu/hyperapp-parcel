{
  "name": "hyperapp-parcel",
  "private": true,
  "version": "0.0.1",
  "description": "A minimal hyperapp boilerplate project with parcel as bundle",
  "keywords": [
    "hyperapp",
    "parceljs",
    "bundle"
  ],
  "author": "ookangzheng",
  "license": "MIT",
  "repository": "ookangzheng/hyperapp-parcel",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel index.html",
    "build": "cross-env NODE_ENV=production parcel build index.html --public-url ./ -d build --no-cache"
  },
  "dependencies": {
    "hyperapp": "^0.18.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.3.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
